{
  "name": "document-management",
  "version": "1.0.0",
  "description": "document management",
  "author": "Yasushi Kobayashi <>",
  "homepage": "http://admin.attela.jp/",
  "main": "src/index.jsx",
  "repository": {
    "type": "git",
